@*Costco Index*@

@section styles{
    @Styles.Render("~/Content/style")
    @Styles.Render("~/Content/costco")
}

<div id="costco-container" class="costco-container" ng-app="costco" ng-controller="CostcoCtrl">
    <div id="cc-head">
        Roadwire
    </div>
    <div ng-view id="costco-view" class="costco-view"></div>
    <footer class="costco-footer">
        &copy; @DateTime.Now.Year - Roadwire LLC
    </footer>
</div>


@section scripts{
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/costco")

    <script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.16/angular-animate.js"></script>

    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBvV2XB3ToYVHeUNMbTrr2Otq7ngt5pHD8&sensor=true&libraries=geometry"> </script>
    @Scripts.Render("~/bundles/maps")
}
